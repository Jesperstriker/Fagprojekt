import static org.junit.Assert.*;
import java.util.List;
import org.junit.Test;

import API.UserAccount;
import API_tests.sampleDataSetup;


public class testUserAccount extends sampleDataSetup{
	@Test
	public void testLogin() throws Exception {
		UserAccount ussAcc = new UserAccount();
		
		String pw = "adminPW";
		String userName = "admin";
		String ownerName = "Michael";
		
		
		assertEquals(ussAcc.getPW(),pw);
		assertEquals(ussAcc.getUserName(),userName);
		assertEquals(ussAcc.getOwnerName(),ownerName);
		
		
		
	}

}
